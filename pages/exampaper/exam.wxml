ionOptions}}" wx:for-item="option" wx:for-index="index" wx:key="index"><block wx:if="{{item.examquestionTypes==2}}"><view class="answer-item data-v-2f8a671b"><checkbox value="{{item.index}}" checked="{{option.checked}}" data-event-opts="{{[['tap',[['itemCheckTap',['$0',index],[[['item.examquestionOptions','',index,'checked']]]]]]]}}" bindtap="__e" class="data-v-2f8a671b"></checkbox><text class="text data-v-2f8a671b">{{option.code+". "+option.text}}</text></view></block></block><block wx:if="{{!isSubmit}}"><button class="btn-submit data-v-2f8a671b" type="primary" data-event-opts="{{[['tap',[['submitTap',['$event']]]]]}}" bindtap="__e">提交答案</button></block><block wx:if="{{isSubmit&&currentItem<=list.length}}"><button class="btn-next data-v-2f8a671b" type="warn" data-event-opts="{{[['tap',[['nextTap',['$event']]]]]}}" bindtap="__e">下一题</button></block></view><block wx:if="{{isSubmit&&currentItem<=list.length}}"><view class="tip-container data-v-2f8a671b"><view class="par data-v-2f8a671b">{{'您的答案是：'+answer+''}}</view></view></block></view></view>